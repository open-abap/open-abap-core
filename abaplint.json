{
  "global": {
    "files": "/src/**/*.*",
    "skipGeneratedGatewayClasses": true,
    "skipGeneratedPersistentClasses": true,
    "skipGeneratedFunctionGroups": true
  },
  "syntax": {
    "version": "v702",
    "errorNamespace": "."
  },
  "rules": {
    "abapdoc": false,
    "allowed_object_naming": false,
    "allowed_object_types": false,
    "ambiguous_statement": true,
    "begin_single_include": true,
    "call_transaction_authority_check": true,
    "7bit_ascii": true,
    "chain_mainly_declarations": true,
    "check_comments": false,
    "check_no_handler_pragma": true,
    "definitions_top": true,
    "begin_end_names": true,
    "xml_consistency": true,
    "check_abstract": true,
    "parser_missing_space": true,
    "colon_missing_space": true,
    "contains_tab": true,
    "empty_statement": true,
    "double_space": true,
    "exporting": true,
    "max_one_statement": true,
    "tabl_enhancement_category": true,
    "check_ddic": true,
    "implement_methods": true,
    "indentation": true,
    "keyword_case": true,
    "when_others_last": true,
    "newline_between_methods": true,
    "avoid_use": {
      "define": true,
      "describeLines": true,
      "break": true,
      "statics": true,
      "defaultKey": false
    },
    "parser_error": true,
    "unknown_types": true,
    "global_class": true,
    "check_syntax": true,
    "functional_writing": true,
    "obsolete_statement": {
      "refresh": true,
      "compute": true,
      "requested": true,
      "communication": true,
      "parameter": true,
      "ranges": true,
      "setExtended": true,
      "withHeaderLine": true,
      "fieldSymbolStructure": true,
      "load": true,
      "typePools": true,
      "occurs": true,
      "add": false,
      "divide": false,
      "move": false,
      "multiply": false,
      "subtract": false
    },
    "forbidden_identifier": {
      "check": [
        "^abstract$",	"^arguments$", "^await$", "^boolean$",
        "^break$",	"^byte$", "^case$", "^catch$",
        "^char$",	"^class$", "^const$", "^continue$",
        "^debugger$",	"^default$", "^delete$", "^do$",
        "^double$",	"^else$", "^enum$", "^eval$",
        "^export$",	"^extends$", "^false$", "^final$",
        "^finally$",	"^float$", "^for$", "^function$",
        "^goto$",	"^if$", "^implements$", "^import$",
        "^instanceof$", "^interface$",
        "^let$",	"^long$", "^native$", "^new$",
        "^null$",	"^package$", "^private$", "^protected$",
        "^public$",	"^return$", "^short$", "^static$",
        "^switch$", "^synchronized$", "^this$",
        "^throw$",	"^throws$", "^transient$", "^true$",
        "^try$",	"^typeof$", "^var$", "^void$",
        "^volatile$",	"^while$", "^with$", "^yield$",
        "^unique\\d+$"]
    }
  }
}